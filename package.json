{
  "name": "chatgpt-that",
  "type": "module",
  "version": "0.0.2",
  "main": "index.js",
  "scripts": {
    "copy-chrome-extension-node-modules": "cp -r packages/chrome-extension/node_modules packages/chrome-extension/___extension/node_modules",
    "build:web-app-tailwind": "npx tailwindcss -i packages/web-app/public/styles/input.css -o packages/web-app/public/styles/output.css -c packages/web-app/tailwind.config.js",
    "build:web-app-tailwind-w": "npx tailwindcss -i packages/web-app/public/styles/input.css -o packages/web-app/public/styles/output.css -c packages/web-app/tailwind.config.js --watch",
    "build:web-app": "webpack --mode production --config packages/web-app/webpack.config.cjs",
    "build:web-app-w": "webpack --mode production --config packages/web-app/webpack.config.cjs -w",
    "build:web-app-backend": "npx tsc -p packages/web-app-backend/tsconfig.json",
    "build:web-app-backend-w": "npx tsc -p packages/web-app-backend/tsconfig.json --watch",
    "build:chrome-extension": "npm run copy-chrome-extension-node-modules && npx tsc -p packages/chrome-extension/tsconfig.json",
    "build:chrome-extension-w": "npm run copy-chrome-extension-node-modules && npx tsc -p packages/chrome-extension/tsconfig.json --watch",
    "start": "concurrently \"npm run build:web-app-tailwind-w\" \"npm run build:web-app-w\" \"npm run build:web-app-backend-w\" \"npx nodemon packages/web-app-backend/___build/index.js\"",
    "start:prod": "npm run build:web-app && npm run build:web-app-backend && node packages/web-app-backend/___build/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.7.7",
    "concurrently": "^9.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "jwt-decode": "^4.0.0",
    "mongoose": "^8.8.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.0.1",
    "stripe": "^17.4.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^22.9.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "tailwindcss": "^3.4.15",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.3",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4"
  }
}
